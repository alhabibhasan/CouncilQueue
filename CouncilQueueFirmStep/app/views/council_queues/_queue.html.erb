<h2 class="tableCaption">Queue</h2>
        <table class="table table-responsive">
            <thead>
                <tr>
                    <th>#</th>
                    <th>Type</th>
                    <th>Name</th>
                    <th>Service</th>
                    <th>Queued at</th>
                    <th colspan="3"></th>
                </tr>
            </thead>
            <tbody>
                <% var = 1 %>
                <% @council_queues.each do |p| %>
                    <tr>
                        <td><%=var%></td>
                        <td>
                            <%= p.visitor_type %>
                        </td>
                        <td>
                            <%= p.Name %>
                        </td>
                        <td>
                            <%= p.service %>
                        </td>
                        <td>
                            <%= p.created_at.to_s[10..15] %>
                        </td>
                    </tr>
                    <% var = var + 1 %>
                    <% end %>
            </tbody>
        </table>